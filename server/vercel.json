{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "functions": {
        "api/user/getinfo/:username": {
          "runtime": "nodejs14",
          "maxDuration": 10,
          "regions": ["all"]
        },
        "api/user/:username/:randomUser": {
          "runtime": "nodejs14",
          "maxDuration": 10,
          "regions": ["all"]
        }
      }
}